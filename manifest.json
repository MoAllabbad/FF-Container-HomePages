{
  "name": "Container Home Pages",
  "version": "1.3.1",
  "manifest_version": 2,
  "incognito": "not_allowed",
  "description": "Specify a default page for each container.",
  "homepage_url": "https://github.com/MoAllabbad/FF-Container-Home-Pages",
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "permissions": [
    "contextualIdentities",
    "storage",
    "cookies",
    "tabs"
  ],
  "background": {
    "page": "js/background/index.html"
  },
  "applications": {
    "gecko": {
      "id": "@container-default-pages",
      "strict_min_version": "67.0"
    }
  },
  "icons": {
		"1024": "img/logo.png"
	}
}
